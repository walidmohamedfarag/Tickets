    <script>
        // إعدادات عامة للتوستر
        toastr.options = {
            "closeButton": true,
            "progressBar": true,
            "positionClass": "toast-top-right",
            "timeOut": "4000"
        };

        // دالة لتطبيق ألوان التوستر حسب الثيم الحالي
        function applyToastrTheme() {
            const isDark = document.documentElement.getAttribute("data-bs-theme") === "dark";

            // إزالة أي استايل قديم
            document.getElementById("toastr-theme-style")?.remove();

            // إنشاء ستايل جديد
            const style = document.createElement("style");
            style.id = "toastr-theme-style";
            style.innerHTML = isDark ? `
                #toast-container > .toast {
                    background-color: #2c2c2c !important;
                    color: #fff !important;
                }
                #toast-container > .toast-success { background-color: #198754 !important; }
                #toast-container > .toast-error { background-color: #dc3545 !important; }
                #toast-container > .toast-info { background-color: #0dcaf0 !important; }
                #toast-container > .toast-warning {
                    background-color: #ffc107 !important;
                    color: #212529 !important;
                }
            ` : `
                #toast-container > .toast {
                    background-color: #fff !important;
                    color: #212529 !important;
                    box-shadow: 0 0 10px rgba(0,0,0,0.1);
                }
                #toast-container > .toast-success { background-color: #d1e7dd !important; color: #0f5132 !important; }
                #toast-container > .toast-error { background-color: #f8d7da !important; color: #842029 !important; }
                #toast-container > .toast-info { background-color: #cff4fc !important; color: #055160 !important; }
                #toast-container > .toast-warning { background-color: #fff3cd !important; color: #664d03 !important; }
            `;
            document.head.appendChild(style);
        }

        // تطبيق أولي حسب الثيم الحالي
        applyToastrTheme();

        // مراقبة تغيّر الثيم في حال المستخدم بدّله من الإعدادات
        const observer = new MutationObserver(applyToastrTheme);
        observer.observe(document.documentElement, { attributes: true, attributeFilter: ['data-bs-theme'] });
    </script>

